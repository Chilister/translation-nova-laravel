{
    "name": "chilister/nova-translation-manager",
    "description": "Nova Translation Manager",
    "keywords": [
        "laravel",
        "nova",
        "nova4"
    ],
    "license": "MIT",
    "require": {
        "php": "^10.0",
        "laravel/nova": ">=4.0"
    },
    "autoload": {
        "psr-4": {
            "Rzakhanov\\Translation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chilister\\Translation\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
